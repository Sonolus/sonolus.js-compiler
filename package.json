{
  "name": "sonolus.js-compiler",
  "version": "1.2.1",
  "description": "Sonolus.js Compiler",
  "author": "NonSpicyBurrito",
  "repository": "github:Sonolus/sonolus.js-compiler",
  "license": "MIT",
  "keywords": [
    "Sonolus"
  ],
  "type": "module",
  "exports": {
    "./shared": "./dist/index.shared.js",
    "./play": "./dist/index.play.js",
    "./tutorial": "./dist/index.tutorial.js",
    "./internal/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "tsc -p . --noEmit && eslint --ext .ts ./src",
    "build": "tsc -p ."
  },
  "dependencies": {
    "acorn": "^8.10.0"
  },
  "peerDependencies": {
    "sonolus-core": "~7.2.0"
  },
  "devDependencies": {
    "@types/estree": "^1.0.1",
    "@types/node": "^16.18.40",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "~5.1.6"
  }
}
