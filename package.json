{
  "name": "@sonolus/sonolus.js-compiler",
  "version": "1.6.6",
  "description": "Sonolus.js Compiler",
  "author": "NonSpicyBurrito",
  "repository": "github:Sonolus/sonolus.js-compiler",
  "license": "MIT",
  "keywords": [
    "Sonolus"
  ],
  "type": "module",
  "exports": {
    "./shared": "./dist/index.shared.js",
    "./play": "./dist/index.play.js",
    "./watch": "./dist/index.watch.js",
    "./preview": "./dist/index.preview.js",
    "./tutorial": "./dist/index.tutorial.js",
    "./internal/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "check-type": "tsc -p . --noEmit",
    "check-lint": "eslint .",
    "check-format": "prettier . --check",
    "build": "tsc -p ."
  },
  "dependencies": {
    "acorn": "^8.14.1"
  },
  "peerDependencies": {
    "@sinclair/typebox": "^0.34.33",
    "@sonolus/core": "~7.13.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/estree": "^1.0.7",
    "@types/node": "^20.17.52",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0"
  }
}
